{
  "name": "",
  "version": "0.1.0",
  "description": "Universal JS app for Bet",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://osman_erdem_asga@bitbucket.org/osman_erdem_asga/betCalculator.git"
  },
  "config": {
    "logLevel": "debug",
    "port": 8080
  },
  "scripts": {
    "start": "STATIC_DIRECTORY=static NODE_ENV=production PORT=8086 LOG_LEVEL=error forever --minUptime=1000 --spinSleepTime=1000 ./server.js",
    "dev": "webpack --config webpack.dev.config.js && npm run devServers",
    "devServers": "node server & node webpack.dev.server.js",
    "pretest": "npm run lint",
    "test": "mocha --compilers js:babel-core/register --recursive",
    "lint": "npm run eslint",
    "csslint": "scss-lint",
    "eslint": "eslint . --ext .js",
    "prepackage": "npm run test",
    "package": "npm run buildProdClient && npm run buildProdServer && npm run tar",
    "buildProdClient": "STATIC_DIRECTORY=static NODE_ENV=production webpack -p --config webpack.prod.client.config.js",
    "buildProdServer": "STATIC_DIRECTORY=static NODE_ENV=production webpack --config webpack.prod.server.config.js",
    "postbuildProdServer": "cd betCalculator && npm install --production",
    "tar": "tar -cjf betCalculator-`echo ${npm_package_version}`.tar.bz2 betCalculator",
    "posttar": "rm -rf betCalculator",
    "prod": "npm run start"
  },
  "dependencies": {
    "body-parser": "^1.14.1",
    "colors": "^1.1.2",
    "compression": "^1.6.1",
    "enzyme": "^2.3.0",
    "express": "^4.13.3",
    "forever": "^0.15.1",
    "jsnlog": "^2.16.0",
    "jsnlog-nodejs": "^2.16.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-number-input": "^15.0.0-rc2",
    "react-redux": "^4.2.01",
    "react-router": "^2.0.0",
    "react-router-scroll": "^0.2.0",
    "redux": "^3.2.1",
    "redux-actions": "^0.9.1",
    "redux-thunk": "^2.0.1",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "babel-core": "^6.4.5",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.1.0",
    "babel-plugin-transform-react-constant-elements": "^6.5.0",
    "babel-plugin-transform-react-inline-elements": "^6.5.0",
    "babel-polyfill": "^6.2.4",
    "babel-preset-es2015": "^6.2.4",
    "babel-preset-react": "^6.2.4",
    "babel-preset-stage-2": "^6.2.4",
    "babel-runtime": "^6.2.4",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^3.0.0",
    "css-loader": "^0.23.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "mocha": "^2.5.3",
    "node-sass": "^3.7.0",
    "npm-check-updates": "^2.5.7",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "redux-mock-store": "^1.0.4",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  }
}
